&ethernet {
	mtd-mac-address = <&factory 0x10>;
	mediatek,portmap = "llllw";
};

&gsw {
	mediatek,port4-gmac;
	mediatek,ephy-base = /bits/ 8 <8>;
};

&sdhci {
	status = "okay";
};

&pcie {
	status = "okay";
};

&ehci {
	status = "okay";
};

&ohci {
	status = "okay";
};

&gpio0 {
	status = "okay";
};

&gpio1 {
	status = "okay";
};

&gpio2 {
	status = "okay";
};

&gpio3 {
	status = "okay";
};

&i2c {
	status = "okay";

	pcf8563: pcf8563@51 {
		compatible = "haoyu,hym8563";
		status = "okay";
		reg = <0x51>;
	};

	pca955500: pca95550@24 {
		compatible = "nxp,pca9555";
		status = "okay";
		reg = <0x24>;
	};
};
